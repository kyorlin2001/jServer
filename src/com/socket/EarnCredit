package com.socket;

/**
 *
 * @author llt
 */
public class EarnCredit extends Decorate
{
   
    Decorate decorate;
    String description = "You won the credit! ";
    
 
    public EarnCredit()
    {
     
        this.decorate = decorate;
    
    }
   public  String getPrize()
   {
   
     String prize = "Won the credit";
    
     return prize;
           
   
   };
   
   public String getDescription()
   {
     return description;
   
   }
   
  
   
    
    
}
